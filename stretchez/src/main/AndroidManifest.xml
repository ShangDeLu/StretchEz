<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="me.shangdelu.stretchez">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="me.shangdelu.stretchez.StretchEzApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.StretchEz"

        android:usesCleartextTraffic="true"
        >
        <activity
            android:name="me.shangdelu.stretchez.WorkOutFragment"
            android:exported="true"
            tools:ignore="Instantiatable"/>
        <activity android:name="me.shangdelu.stretchez.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>